<view class="score-scroll-ico"><block a:for="{{yelloScore}}" a:for-item="yello" a:for-index="index" a:key="index"><image class="score-scroll-ico-star" src="../static/icos/star-yellow.png"></image></block><block a:for="{{grayScore}}" a:for-item="gray" a:for-index="index" a:key="index"><image class="score-scroll-ico-star" src="../static/icos/star-gray.png"></image></block><block a:if="{{showScore==1}}"><view class="score-scroll-ico-comment">{{'评分:'+innerScore+''}}</view></block></view>