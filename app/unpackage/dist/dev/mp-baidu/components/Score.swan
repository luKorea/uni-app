<view class="score-scroll-ico"><block s-for="{{yelloScore}}" s-for-item="yello" s-for-index="index" s-key="index"><image class="score-scroll-ico-star" src="../static/icos/star-yellow.png"></image></block><block s-for="{{grayScore}}" s-for-item="gray" s-for-index="index" s-key="index"><image class="score-scroll-ico-star" src="../static/icos/star-gray.png"></image></block><block s-if="{{showScore==1}}"><view class="score-scroll-ico-comment">{{'评分:'+innerScore+''}}</view></block></view>